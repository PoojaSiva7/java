import java .util.Scanner;
 public class hello{
   public static void main(String args[])
   {
       Scanner scan=new Scanner(System.in);
       int n=scan.nextInt();
       for(int i=1;i<=4;i++){
   for(int j=1;j<=4;j++){
             if(i==1||j==4||i==4||j==1){
    System.out.print("*");
    }
else{
   System.out.print(" ");
       }

}
System.out.println("");
}
}
}

