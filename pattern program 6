import java .util.Scanner;
 public class hello{
    public static void main(String args[])
    {
        Scanner scan=new Scanner(System.in);
        int n=scan.nextInt();   
for( int i=1;i<=n;i++){
        for( int j=1;j<=n;j++)
        System.out.print("*");
        System.out.println();
        }
	   for(char i='z';i>='a';i--)
        System.out.print(i+" ");
    }
}

