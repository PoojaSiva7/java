import java. util.Scanner;
public class basic{
    public static void main(String args[]){
        Scanner scan=new Scanner(System.in);
        char n=scan.next().charAt(0);
        for(char i='A';i<='C';i++){
            for(char j='A';j<='C';j++){
                System.out.print(j);
            }
            System.out.println("");
        }
    }
}
